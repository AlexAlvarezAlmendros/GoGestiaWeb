VITE_API_URL=https://api.gogenaiweb.com
VITE_SITE_NAME=GoGenAI
VITE_CONTACT_EMAIL=contacto@gogenaiweb.com

# Auth0 Configuration
# Obtén estos valores desde tu dashboard de Auth0

# Dominio de Auth0 (ej: dev-12345678.us.auth0.com)
VITE_AUTH0_DOMAIN=your-auth0-domain.auth0.com

# Client ID de tu aplicación Auth0
VITE_AUTH0_CLIENT_ID=your-auth0-client-id

# Audience de tu API (opcional, pero recomendado para JWT)
VITE_AUTH0_AUDIENCE=your-api-audience

# URL base de tu API
VITE_API_BASE_URL=http://localhost:3000

# Entorno (development, production)
VITE_NODE_ENV=development